{
  "name": "kleverbot-backend",
  "version": "1.0.0",
  "description": "KleverBot AI Agent Management Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:check": "eslint src/**/*.js",
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "create-admin": "node scripts/create-admin.js",
    "backup": "node scripts/backup.js",
    "build": "echo Build process here",
    "deploy": "node scripts/deploy.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "model:generate": "npx sequelize-cli model:generate",
    "seed:generate": "npx sequelize-cli seed:generate",
    "migration:generate": "npx sequelize-cli migration:generate"
  },
  "keywords": [
    "kleverbot",
    "ai",
    "agent",
    "management",
    "api"
  ],
  "author": "KleverBot Team",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "juice": "^11.0.3",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.4",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.7",
    "nodemailer-express-handlebars": "^7.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "sequelize": "^6.37.7",
    "uui": "^1.0.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "_moduleAliases": {
    "@": "./src",
    "@/config": "./src/config",
    "@/controllers": "./src/controllers",
    "@/services": "./src/services",
    "@/repositories": "./src/repositories",
    "@/middleware": "./src/middleware",
    "@/routes": "./src/routes",
    "@/utils": "./src/utils",
    "@/validators": "./src/validators",
    "@/models": "./src/database/sequelize-models",
    "@/database": "./src/database"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "axios": "^1.12.2",
    "eslint": "^8.56.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3"
  }
}
